{
  "name": "@stageit-labs/rspack-config",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@stageit-labs/core": "workspace:*"
  },
  "peerDependencies": {
    "@rspack/core": ">=1.0.0",
    "@rspack/plugin-react-refresh": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "@rspack/core": {
      "optional": true
    },
    "@rspack/plugin-react-refresh": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rslib/core": "^0.0.5",
    "@rspack/core": "^1.5.1",
    "@rspack/plugin-react-refresh": "^1.4.3",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "@repo/test-utils": "workspace:*"
  }
}
