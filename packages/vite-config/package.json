    "vite": {
      "optional": true
    },
    "@vitejs/plugin-react": {
      "optional": true
    },
    "@vitejs/plugin-react-swc": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rslib/core": "^0.0.5",
    "@vitejs/plugin-react": "^4.3.2",
    "@types/node": "^24.3.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "@repo/test-utils": "workspace:*"
  }
